@prefix : <http://streamreasoning.org/ontologies/vocals-sd##> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://streamreasoning.org/ontologies/vocals-sd#> .

<http://streamreasoning.org/ontologies/vocals-sd#> rdf:type owl:Ontology ;
                                   owl:versionIRI <http://streamreasoning.org/ontologies/vocals-sd> ;
                                   <http://purl.org/dc/elements/1.1/creator> <http://w3id.org/people/jpcik/me> ,
                                                                             <http://streamreasoning.org/ontologies/vocals#RiccardoTommasini> ,
                                                                             <http://streamreasoning.org/ontologies/vocals#YehiaAboSedira> ;
                                   <http://purl.org/dc/elements/1.1/publisher> <http://aislab.hevs.ch/> ;
                                   <http://purl.org/dc/terms/created> "2017-12-23T13:00:00"^^xsd:dateTime ;
                                   <http://purl.org/dc/terms/description> "Cette ontologie a été conçue pour representer des services qui genèrent ou consomment données et meta-données de streams RDF, ainsi que leurs points d'accès."@fr ,
                                                                          "Esta ontología has sido diseñada para representar servicios que proveen o manipulan Streams de datos en RDF, así como los endpoints a través de los cuales se puede acceder a los datos de estos streams;  y los meta datos correspondientes."@es ,
                                                                          "This ontology aims to model Services for publishing and consuming RDF streams, their metadata, and access endpoints for publishing and consuming these streams"@en ;
                                   <http://purl.org/dc/terms/issued> "2017-12-29"^^xsd:date ;
                                   <http://purl.org/dc/terms/license> <https://creativecommons.org/licenses/by/4.0/> ;
                                   <http://purl.org/dc/terms/modified> "2017-12-30"^^xsd:date ;
                                   <http://purl.org/dc/terms/publisher> <http://w3id.org/hes-so/aislab> ;
                                   <http://purl.org/dc/terms/rights> "Copyright © 2017 Politecnico di Milano & HES-SO Valais-Wallis"^^xsd:string ;
                                   <http://purl.org/dc/terms/title> "VoCaLS-SD: A Vocabulary and Catalog for Linked Streams, Service Description Module"@en ,
                                                                    "VoCaLS-SD: Vocabulario y catálogo para Streams enlazados, módulo de descripción de servicios"@es ,
                                                                    "VoCaLS: ontologie de representation de services pour données et metadonnées de streams RDF"@fr ;
                                   <http://purl.org/vocab/vann/preferredNamespacePrefix> "vocals-sd"^^xsd:string ;
                                   <http://purl.org/vocab/vann/preferredNamespaceUri> "http://streamreasoning.org/ontologies/vocals-sd#"^^xsd:string ;
                                   rdfs:comment "Cette ontologie a été conçue pour representer des services qui genèrent ou consomment données et meta-données de streams RDF, ainsi que leurs points d'accès."@fr ,
                                                "Esta ontología has sido diseñada para representar servicios que proveen o manipulan Streams de datos en RDF, así como los endpoints a través de los cuales se puede acceder a los datos de estos streams;  y los meta datos correspondientes."@es ,
                                                "This ontology aims to model Services for publishing and consuming RDF streams, their metadata, and access endpoints for publishing and consuming these streams"@en ;
                                   owl:versionInfo "0.1.1"^^xsd:string .

#################################################################
#    Annotation properties
#################################################################

###  http://streamreasoning.org/ontologies/vocals-sd#name
<http://streamreasoning.org/ontologies/vocals-sd#name> rdf:type owl:AnnotationProperty ;
                                      rdfs:comment "Relates a named time varying graph to the name by which it may be referenced in a FROM/FROM NAMED clause. The object of the name property is an IRI."^^xsd:string ;
                                      rdfs:domain <http://streamreasoning.org/ontologies/vocals-sd#TimeVaryingGraph> .


#################################################################
#    Object Properties
#################################################################

###  http://streamreasoning.org/ontologies/vocals-sd#availableGraph
<http://streamreasoning.org/ontologies/vocals-sd#availableGraph> rdf:type owl:ObjectProperty ;
                                                rdfs:domain <http://streamreasoning.org/ontologies/vocals-sd#RDFStreamService> ;
                                                rdfs:range <http://streamreasoning.org/ontologies/vocals-sd#TimeVaryingGraph> ;
                                                rdfs:comment "Relates an instance of RDFStreamService to the time varying graphs available by the RDFStreamService."@en ;
                                                rdfs:label "availableGraph"@en .


###  http://streamreasoning.org/ontologies/vocals-sd#availableStream
<http://streamreasoning.org/ontologies/vocals-sd#availableStream> rdf:type owl:ObjectProperty ;
                                                 rdfs:subPropertyOf owl:topObjectProperty ;
                                                 rdfs:domain <http://streamreasoning.org/ontologies/vocals-sd#RDFStreamService> ;
                                                 rdfs:range <http://streamreasoning.org/ontologies/vocals#RDFStream> ;
                                                 rdfs:comment "Relates an RDFStreamService with the RDF streams available in the service"@en ;
                                                 rdfs:label "availableStream"@en .


###  http://streamreasoning.org/ontologies/vocals-sd#defaultGraph
<http://streamreasoning.org/ontologies/vocals-sd#defaultGraph> rdf:type owl:ObjectProperty ;
                                              rdfs:domain <http://streamreasoning.org/ontologies/vocals-sd#StreamingDataset> ;
                                              rdfs:range <http://streamreasoning.org/ontologies/vocals-sd#TimeVaryingGraph> ;
                                              rdfs:comment "Relates an instance of a Streaming Data Set to the description of its default graph."@en ;
                                              rdfs:label "defaultGraph"@en .


###  http://streamreasoning.org/ontologies/vocals-sd#defaultStreamingDataset
<http://streamreasoning.org/ontologies/vocals-sd#defaultStreamingDataset> rdf:type owl:ObjectProperty ;
                                                         rdfs:domain <http://streamreasoning.org/ontologies/vocals-sd#RDFStreamService> ;
                                                         rdfs:range <http://streamreasoning.org/ontologies/vocals-sd#StreamingDataset> ;
                                                         rdfs:comment "Relates an instance of an RDFStreamService to a description of the default Streaming Dataset available when no explicit dataset is specified."@en ;
                                                         rdfs:label "defaultStreamingDataset"@en .


###  http://streamreasoning.org/ontologies/vocals-sd#describedBy
<http://streamreasoning.org/ontologies/vocals-sd#describedBy> rdf:type owl:ObjectProperty ;
                                             rdfs:domain <http://streamreasoning.org/ontologies/vocals#RDFStream> ;
                                             rdfs:range <http://streamreasoning.org/ontologies/vocals-sd#CatalogService> ;
                                             rdfs:comment "Points to an instance of a CatalogingService that describes a certain RDFStream."@en ;
                                             rdfs:label "describedBy"@en .


###  http://streamreasoning.org/ontologies/vocals-sd#endedAt
<http://streamreasoning.org/ontologies/vocals-sd#endedAt> rdf:type owl:ObjectProperty ;
                                         rdfs:domain <http://streamreasoning.org/ontologies/vocals-sd#Window> ;
                                         rdfs:range <http://www.w3.org/2006/time#Instant> ;
                                         rdfs:label "Relates a window and the time instant when it ended"@en ,
                                                    "endedAt"@en .


###  http://streamreasoning.org/ontologies/vocals-sd#graph
<http://streamreasoning.org/ontologies/vocals-sd#graph> rdf:type owl:ObjectProperty ;
                                       rdfs:range <http://streamreasoning.org/ontologies/vocals-sd#TimeVaryingGraph> ;
                                       rdfs:comment "Relates a named time varying graph to its graph description."^^xsd:string ;
                                       rdfs:label "graph"@en .


###  http://streamreasoning.org/ontologies/vocals-sd#hasFeature
<http://streamreasoning.org/ontologies/vocals-sd#hasFeature> rdf:type owl:ObjectProperty ;
                                            rdfs:domain <http://streamreasoning.org/ontologies/vocals-sd#RDFStreamService> ;
                                            rdfs:range <http://streamreasoning.org/ontologies/vocals-sd#RDFStreamingFeature> ;
                                            rdfs:comment "Relates an RDF Stream Service to an available RDF Streaming Feature. This relationship describe the features that the service provides."@en ;
                                            rdfs:label "hasFeature"@en .


###  http://streamreasoning.org/ontologies/vocals-sd#namedTimeVaryingGraph
<http://streamreasoning.org/ontologies/vocals-sd#namedTimeVaryingGraph> rdf:type owl:ObjectProperty ;
                                                       rdfs:domain <http://streamreasoning.org/ontologies/vocals-sd#StreamingDataset> ;
                                                       rdfs:range <http://streamreasoning.org/ontologies/vocals-sd#TimeVaryingGraph> ;
                                                       rdfs:comment "Relates an instance of StreamingDataset to the description of one of its named time varying graphs. The description of such a named time varying graph MUST include the name property."@en ;
                                                       rdfs:label "namedTimeVaryingGraph"@en .


###  http://streamreasoning.org/ontologies/vocals-sd#publishedBy
<http://streamreasoning.org/ontologies/vocals-sd#publishedBy> rdf:type owl:ObjectProperty ;
                                             rdfs:domain <http://streamreasoning.org/ontologies/vocals#RDFStream> ;
                                             rdfs:range <http://streamreasoning.org/ontologies/vocals-sd#PublishingService> ;
                                             rdfs:comment "Points to an instance of PublishingService that publishs a certain (or certains) RDFStream."@en ;
                                             rdfs:label "publishedBy"@en .


###  http://streamreasoning.org/ontologies/vocals-sd#registerdTask
<http://streamreasoning.org/ontologies/vocals-sd#registerdTask> rdf:type owl:ObjectProperty ;
                                               rdfs:domain <http://streamreasoning.org/ontologies/vocals-sd#RDFStreamService> ;
                                               rdfs:comment "Relates an instance of RDFStreamService with instances of running tasks, e.g. RSP-QL Query, other RDF stream processing tasks"@en ;
                                               rdfs:label "registerdTask"@en .


###  http://streamreasoning.org/ontologies/vocals-sd#registeredBy
<http://streamreasoning.org/ontologies/vocals-sd#registeredBy> rdf:type owl:ObjectProperty ;
                                              rdfs:domain <http://streamreasoning.org/ontologies/vocals#RDFStream> ;
                                              rdfs:range <http://streamreasoning.org/ontologies/vocals-sd#ProcessingService> ;
                                              rdfs:comment "Points to an instance of ProcessingService that registers/ processes a certain RDFStream."@en ;
                                              rdfs:label "registeredBy"@en .


###  http://streamreasoning.org/ontologies/vocals-sd#registeredStreams
<http://streamreasoning.org/ontologies/vocals-sd#registeredStreams> rdf:type owl:ObjectProperty ;
                                                   rdfs:domain <http://streamreasoning.org/ontologies/vocals-sd#RDFStreamService> ;
                                                   rdfs:range <http://streamreasoning.org/ontologies/vocals#RDFStream> ;
                                                   rdfs:comment "Relates an instance of RDFStreamService to the Stream(s) registered on it."@en ;
                                                   rdfs:label "registeredStream"@en .


###  http://streamreasoning.org/ontologies/vocals-sd#resultFormat
<http://streamreasoning.org/ontologies/vocals-sd#resultFormat> rdf:type owl:ObjectProperty ;
                                              rdfs:domain <http://streamreasoning.org/ontologies/vocals-sd#RDFStreamService> ;
                                              rdfs:range <http://www.w3.org/ns/formats/Format> ;
                                              rdfs:comment "Relates an instance of RDFStreamService to a format that is supported for serializing streaming results."@en ;
                                              rdfs:label "resultFormat"@en .


###  http://streamreasoning.org/ontologies/vocals-sd#rspEndpoint
<http://streamreasoning.org/ontologies/vocals-sd#rspEndpoint> rdf:type owl:ObjectProperty ;
                                             rdfs:domain <http://streamreasoning.org/ontologies/vocals-sd#RDFStreamService> ;
                                             rdfs:comment "The RSP-QL endpoint of an vocals:RSPService that implements the RSP-QL Protocol service. The object of the rspEndpoint property is an IRI."@en ;
                                             rdfs:label "rspEndpoint"@en .


###  http://streamreasoning.org/ontologies/vocals-sd#startedAt
<http://streamreasoning.org/ontologies/vocals-sd#startedAt> rdf:type owl:ObjectProperty ;
                                           rdfs:domain <http://streamreasoning.org/ontologies/vocals-sd#Window> ;
                                           rdfs:range <http://www.w3.org/2006/time#Instant> ;
                                           rdfs:comment "Relates a window and the time instant when it started"@en ;
                                           rdfs:label "startedAt"@en .


#################################################################
#    Classes
#################################################################

###  http://www.w3.org/2006/time#Instant
<http://www.w3.org/2006/time#Instant> rdf:type owl:Class .


###  http://www.w3.org/ns/dcat#Dataset
<http://www.w3.org/ns/dcat#Dataset> rdf:type owl:Class ;
                                    owl:disjointWith <http://streamreasoning.org/ontologies/vocals-sd#StreamingDataset> .


###  http://www.w3.org/ns/formats/Format
<http://www.w3.org/ns/formats/Format> rdf:type owl:Class .


###  http://streamreasoning.org/ontologies/vocals#RDFStream
<http://streamreasoning.org/ontologies/vocals#RDFStream> rdf:type owl:Class .


###  http://streamreasoning.org/ontologies/vocals-sd#CatalogService
<http://streamreasoning.org/ontologies/vocals-sd#CatalogService> rdf:type owl:Class ;
                                                rdfs:subClassOf <http://streamreasoning.org/ontologies/vocals-sd#RDFStreamService> ;
                                                rdfs:comment "An CatalogService is a service providing access to stream descriptions. (e.g. SPARQL endpoint, RDF filse with stream descriptions, etc.)"@en ;
                                                rdfs:label "CatalogService"@en ;
                                                rdfs:seeAlso "vocals:StreamDescriptor"^^xsd:string .


###  http://streamreasoning.org/ontologies/vocals-sd#Operation
<http://streamreasoning.org/ontologies/vocals-sd#Operation> rdf:type owl:Class ;
                                           rdfs:subClassOf <http://streamreasoning.org/ontologies/vocals-sd#RDFStreamingFeature> ;
                                           rdfs:comment "An operation represents an available action/function that can be performed by an RDF Stream Service. An operation may ingest stream items or static data items, and output either of them."@en ;
                                           rdfs:label "Operation"@en ;
                                           rdfs:seeAlso <http://streamreasoning.org/ontologies/vocals-sd#RDFStreamService> .


###  http://streamreasoning.org/ontologies/vocals-sd#PhysicalWindow
<http://streamreasoning.org/ontologies/vocals-sd#PhysicalWindow> rdf:type owl:Class ;
                                                rdfs:subClassOf <http://streamreasoning.org/ontologies/vocals-sd#Window> ;
                                                rdfs:comment "A PhysicalWindow is a Window defined by an interval size (in units of RDF elements), e.g. a window of n RDF elements."@en ;
                                                rdfs:label "PhysicalWindow"@en ;
                                                rdfs:seeAlso <http://streamreasoning.org/ontologies/vocals-sd#Window> .


###  http://streamreasoning.org/ontologies/vocals-sd#ProcessingService
<http://streamreasoning.org/ontologies/vocals-sd#ProcessingService> rdf:type owl:Class ;
                                                   rdfs:subClassOf <http://streamreasoning.org/ontologies/vocals-sd#RDFStreamService> ;
                                                   rdfs:comment "A ProcessingService represents an RDF stream service that provides processing features over RDF streams. Processing refers to any kind of transformation on streaming data, i.e., querying, reasoning, filtering, etc."@en ;
                                                   rdfs:label "ProcessingService"@en .


###  http://streamreasoning.org/ontologies/vocals-sd#PublishingService
<http://streamreasoning.org/ontologies/vocals-sd#PublishingService> rdf:type owl:Class ;
                                                   rdfs:subClassOf <http://streamreasoning.org/ontologies/vocals-sd#RDFStreamService> ;
                                                   rdfs:comment "A PublishingService represents an RDF stream service for publishing purposes. Publishing can to be interpreted as Linked Data publishing, where possible, but it may support other types of RDF stream publishing mechanisms as well."@en ;
                                                   rdfs:label "PublishingService"@en .


###  http://streamreasoning.org/ontologies/vocals-sd#R2ROperation
<http://streamreasoning.org/ontologies/vocals-sd#R2ROperation> rdf:type owl:Class ;
                                              rdfs:subClassOf <http://streamreasoning.org/ontologies/vocals-sd#Operation> ;
                                              owl:disjointWith <http://streamreasoning.org/ontologies/vocals-sd#R2SOperation> ,
                                                               <http://streamreasoning.org/ontologies/vocals-sd#S2ROperation> ,
                                                               <http://streamreasoning.org/ontologies/vocals-sd#S2SOperation> ;
                                              rdfs:comment "An R2ROperation represents a realtion-to-relation operation that an RDF Stream Service can perform, such as filtering, aggregation, projection, join, etc. It takes as input a relation (e.g. RDF data mappings) and outputs also a relation (e.g. RDF data mappings)."@en ;
                                              rdfs:isDefinedBy "Dell'Aglio, Daniele, et al. \"RSP-QL semantics: a unifying query model to explain heterogeneity of RDF stream processing systems.\" International Journal on Semantic Web and Information Systems (IJSWIS) 10.4 (2014): 17-44."^^xsd:string ;
                                              rdfs:label "R2ROperation"@en .


###  http://streamreasoning.org/ontologies/vocals-sd#R2SOperation
<http://streamreasoning.org/ontologies/vocals-sd#R2SOperation> rdf:type owl:Class ;
                                              rdfs:subClassOf <http://streamreasoning.org/ontologies/vocals-sd#Operation> ;
                                              rdfs:comment "An R2SOperation represents a realtion-to-stream operation that an RDF Stream Service can perform. It takes as input a relation (e.g. RDF data mappings), and ouputs RDF Stream elements, e.g. for instance an operation that replays a static dataset as a stream, or an Istream, Rstream or Dstream operation."@en ;
                                              rdfs:isDefinedBy "Dell'Aglio, Daniele, et al. \"RSP-QL semantics: a unifying query model to explain heterogeneity of RDF stream processing systems.\" International Journal on Semantic Web and Information Systems (IJSWIS) 10.4 (2014): 17-44."^^xsd:string ;
                                              rdfs:label "R2SOperation"@en .


###  http://streamreasoning.org/ontologies/vocals-sd#RDFStreamService
<http://streamreasoning.org/ontologies/vocals-sd#RDFStreamService> rdf:type owl:Class ;
                                                  rdfs:subClassOf <http://streamreasoning.org/ontologies/vocals-sd#StreamingService> ;
                                                  rdfs:comment "An RDFStreamService represents a service that allows producing and/or consuming and/or processing RDF streams, and made available via an RDF streaming protocol."@en ;
                                                  rdfs:label "RDFStreamService"@en .


###  http://streamreasoning.org/ontologies/vocals-sd#RDFStreamingFeature
<http://streamreasoning.org/ontologies/vocals-sd#RDFStreamingFeature> rdf:type owl:Class ;
                                                     rdfs:comment "Represents a feature of a RDF Streaming service. Features may include operations over RDF streams, policies, time handling mechanisms, etc."@en ;
                                                     rdfs:label "RDFStreamingFeature"@en ;
                                                     rdfs:seeAlso <http://streamreasoning.org/ontologies/vocals-sd#RDFStreamService> .


###  http://streamreasoning.org/ontologies/vocals-sd#ReportingPolicy
<http://streamreasoning.org/ontologies/vocals-sd#ReportingPolicy> rdf:type owl:Class ;
                                                 rdfs:subClassOf <http://streamreasoning.org/ontologies/vocals-sd#RDFStreamingFeature> ;
                                                 rdfs:comment "The reporting policy establishes a strategy according to which data items are reported, typically within a window. E.g. report data items when teh window closes, periodically, when the window contents change, etc."@en ;
                                                 rdfs:label "ReportingPolicy"@en ;
                                                 rdfs:seeAlso <http://streamreasoning.org/ontologies/vocals-sd#Window> .


###  http://streamreasoning.org/ontologies/vocals-sd#S2ROperation
<http://streamreasoning.org/ontologies/vocals-sd#S2ROperation> rdf:type owl:Class ;
                                              rdfs:subClassOf <http://streamreasoning.org/ontologies/vocals-sd#Operation> ;
                                              rdfs:comment "An S2ROperation represents a stream-to-relation operation that an RDFStreamService can perform. It takes as input an RDF steam and putputs RDF data mapping, for instance windowing operators."@en ;
                                              rdfs:isDefinedBy "Dell'Aglio, Daniele, et al. \"RSP-QL semantics: a unifying query model to explain heterogeneity of RDF stream processing systems.\" International Journal on Semantic Web and Information Systems (IJSWIS) 10.4 (2014): 17-44."^^xsd:string ;
                                              rdfs:label "S2ROperation"@en .


###  http://streamreasoning.org/ontologies/vocals-sd#S2SOperation
<http://streamreasoning.org/ontologies/vocals-sd#S2SOperation> rdf:type owl:Class ;
                                              rdfs:subClassOf <http://streamreasoning.org/ontologies/vocals-sd#Operation> ;
                                              rdfs:comment "An S2SOperation represents a stream-to-stream operation that an RDFStreamService can perform. It takes as input and output RDF streams (e.g. transformation operations over RDF streams)."@en ;
                                              rdfs:label "S2SOperation"@en .


###  http://streamreasoning.org/ontologies/vocals-sd#SetOperation
<http://streamreasoning.org/ontologies/vocals-sd#SetOperation> rdf:type owl:Class ;
                                              rdfs:subClassOf <http://streamreasoning.org/ontologies/vocals-sd#S2SOperation> ;
                                              rdfs:comment "An instance of vsd:SetOperation represents an available set operator that can be performed by vsd:RSService. (e.g. union, intersection)"^^xsd:string ;
                                              rdfs:label "Set operation"^^xsd:string .


###  http://streamreasoning.org/ontologies/vocals-sd#StreamingDataset
<http://streamreasoning.org/ontologies/vocals-sd#StreamingDataset> rdf:type owl:Class ;
                                                  rdfs:comment "A Streaming Dataset provides the input data (from stream and time-varying graphs) for an RDF stream service. A Streaming Dataset is a set composed by an (optional) default graph, n (n ≥ 0) named graphs and m (m ≥ 0) named time-varying graphs."@en ;
                                                  rdfs:label "StreamingDataset"@en ;
                                                  rdfs:seeAlso <http://streamreasoning.org/ontologies/vocals-sd#RDFStreamService> ,
                                                               <http://streamreasoning.org/ontologies/vocals-sd#TimeVaryingGraph> .


###  http://streamreasoning.org/ontologies/vocals-sd#StreamingService
<http://streamreasoning.org/ontologies/vocals-sd#StreamingService> rdf:type owl:Class ;
                                                  rdfs:comment "A StreamingService represents a service made available via a certain streaming protocol. The service may provide features such as stream processing, publishing, provisioning, etc."@en ;
                                                  rdfs:label "StreamingService"@en .


###  http://streamreasoning.org/ontologies/vocals-sd#TimeBasedWindow
<http://streamreasoning.org/ontologies/vocals-sd#TimeBasedWindow> rdf:type owl:Class ;
                                                 rdfs:subClassOf <http://streamreasoning.org/ontologies/vocals-sd#Window> ;
                                                 rdfs:comment "A TimeBasedWindow is a Window defined in terms of time. More formally, it is a window defined through two time instants o, c (respectively named opening and closing time instants) such that: W(S) = {d|(d, t) ∈ S ∧ t ∈ (o, c]}"@en ;
                                                 rdfs:label "TimeBasedWindow"@en ;
                                                 rdfs:seeAlso <http://streamreasoning.org/ontologies/vocals-sd#Window> .


###  http://streamreasoning.org/ontologies/vocals-sd#TimeControl
<http://streamreasoning.org/ontologies/vocals-sd#TimeControl> rdf:type owl:Class ;
                                             rdfs:subClassOf <http://streamreasoning.org/ontologies/vocals-sd#RDFStreamingFeature> ;
                                             rdfs:comment "TimeControl is a feature that establishes the type of timing control in the RDF stream Service, e.g. time managed internally, externally, etc."@en ;
                                             rdfs:label "TimeControl"@en .


###  http://streamreasoning.org/ontologies/vocals-sd#TimeSemantics
<http://streamreasoning.org/ontologies/vocals-sd#TimeSemantics> rdf:type owl:Class ;
                                               rdfs:subClassOf <http://streamreasoning.org/ontologies/vocals-sd#RDFStreamingFeature> ;
                                               rdfs:comment "An instance of TimeSemantics represent the time domain used by streaming service while handeling the data elements, e.g. event time, porcessing time"@en ;
                                               rdfs:isDefinedBy "The world beyond batch: Streaming 101 (https://www.oreilly.com/ideas/the-world-beyond-batch-streaming-101)"^^xsd:string ;
                                               rdfs:label "TimeSemantics"@en ;
                                               rdfs:seeAlso <http://streamreasoning.org/ontologies/vocals-sd#RDFStreamService> .


###  http://streamreasoning.org/ontologies/vocals-sd#TimeVaryingGraph
<http://streamreasoning.org/ontologies/vocals-sd#TimeVaryingGraph> rdf:type owl:Class ;
                                                  rdfs:comment "A TimeVaryingGraph represents and RDF graph that changes over time. Formally, A time-varying graph G is a function that relates time instants t ∈ T to RDF graphs: G : T → {g | g is an RDF graph}, where T is the time domain."@en ;
                                                  rdfs:label "TimeVaryingGraph"@en .


###  http://streamreasoning.org/ontologies/vocals-sd#Window
<http://streamreasoning.org/ontologies/vocals-sd#Window> rdf:type owl:Class ;
                                        rdfs:subClassOf <http://www.w3.org/ns/dcat#Dataset> ;
                                        rdfs:comment "A Window represents a finite protion of a stream, consisting of a set of RDF statements. Formally, a window W(S) is a set of RDF statements extracted from a stream S."@en ;
                                        rdfs:label "Window"@en ;
                                        rdfs:seeAlso <http://streamreasoning.org/ontologies/vocals#RDFStream> .


#################################################################
#    Individuals
#################################################################

###  http://streamreasoning.org/ontologies/vocals-sd#RSPQL
<http://streamreasoning.org/ontologies/vocals-sd#RSPQL> rdf:type owl:NamedIndividual ,
                                                <http://streamreasoning.org/ontologies/vocals-sd#S2SOperation> .


