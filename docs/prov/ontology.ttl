###  https://w3id.org/rsp/vocals-prov#containedIn
:containedIn rdf:type owl:ObjectProperty ;
             owl:inverseOf :contains ;
             rdfs:domain :Operator ;
             rdfs:range :Task ;
             rdfs:comment "Relates an operator to the containing task ."^^xsd:string .


###  https://w3id.org/rsp/vocals-prov#contains
:contains rdf:type owl:ObjectProperty ;
          rdfs:domain :Task ;
          rdfs:range :Operator ;
          rdfs:comment "Relates task to operators used to finish the task."^^xsd:string .


###  https://w3id.org/rsp/vocals-prov#endedAt
:endedAt rdf:type owl:ObjectProperty ;
         rdfs:domain :Window ;
         rdfs:range <http://www.w3.org/2006/time#Instant> .


###  https://w3id.org/rsp/vocals-prov#followedBy
:followedBy rdf:type owl:ObjectProperty ;
            owl:inverseOf :precededBy ;
            rdfs:domain :Operator ;
            rdfs:range :Operator ;
            rdfs:comment "Points to the following vprov:operator to be applied after the current one in a vprov:Task."^^xsd:string .


###  https://w3id.org/rsp/vocals-prov#hasOutput
:hasOutput rdf:type owl:ObjectProperty ;
           rdfs:domain :Task ;
           rdfs:range <http://www.w3.org/ns/prov#Entity> ;
           rdfs:comment "Points to the output of a vprov:operator to be applied before the current one in a vprov:Task."^^xsd:string .


###  https://w3id.org/rsp/vocals-prov#performs
:performs rdf:type owl:ObjectProperty ;
          rdfs:range <https://w3id.org/rsp/vocals-sd#Operation> ;
          rdfs:isDefinedBy "Relate vsd:Operation to vprov:Operator ."^^xsd:string .


###  https://w3id.org/rsp/vocals-prov#precededBy
:precededBy rdf:type owl:ObjectProperty ;
            rdfs:domain :Operator ;
            rdfs:range :Operator ;
            rdfs:comment "Points to the preceding vprov:operator to be applied before the current one in a vprov:Task."^^xsd:string .


###  https://w3id.org/rsp/vocals-prov#startedAt
:startedAt rdf:type owl:ObjectProperty ;
           rdfs:domain :Window ;
           rdfs:range <http://www.w3.org/2006/time#Instant> .


###  https://w3id.org/rsp/vocals-prov#startingTime
:startingTime rdf:type owl:ObjectProperty ;
              rdfs:domain :WindowOperator ;
              rdfs:range <http://www.w3.org/2006/time#Instant> .


