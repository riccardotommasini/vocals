#################################################################
#    Annotation properties
#################################################################

###  https://w3id.org/rsp/vocals-sd#name
vsd:name rdf:type owl:AnnotationProperty ;
         rdfs:comment "Relates a named time varying graph to the name by which it may be referenced in a FROM/FROM NAMED clause. The object of the vsd:name property is an IRI." ;
         rdfs:domain vsd:TimeVaryingGraph .


#################################################################
#    Object Properties
#################################################################

###  https://w3id.org/rsp/vocals-sd#availableGraphs
vsd:availableGraphs rdf:type owl:ObjectProperty ;
                    rdfs:domain vsd:RSService ;
                    rdfs:range vsd:TimeVaryingGraph ;
                    rdfs:comment "Relates an instance of vsd:RSService to the time varying graphs available by the vsd:RSService." .


###  https://w3id.org/rsp/vocals-sd#defaultGraph
vsd:defaultGraph rdf:type owl:ObjectProperty ;
                 rdfs:domain vsd:SDS ;
                 rdfs:range vsd:TimeVaryingGraph ;
                 rdfs:comment "Relates an instance of vocals:RDFStream to the description of its default graph." .


###  https://w3id.org/rsp/vocals-sd#defaultSDS
vsd:defaultSDS rdf:type owl:ObjectProperty ;
               rdfs:comment "Relates an instance of sd:Service to a description of the default SDS available when no explicit dataset is specified in the query." .


###  https://w3id.org/rsp/vocals-sd#describedBy
vsd:describedBy rdf:type owl:ObjectProperty ;
                rdfs:domain vsd:CatalogService ;
                rdfs:range vocals:RDFStream ;
                rdfs:comment "Points to an instance of vsd:CatalogingService that describes a certain vocals:RDFStream." .


###  https://w3id.org/rsp/vocals-sd#endedAt
vsd:endedAt rdf:type owl:ObjectProperty ;
            rdfs:domain vsd:Window ;
            rdfs:range time:Instant .


###  https://w3id.org/rsp/vocals-sd#graph
vsd:graph rdf:type owl:ObjectProperty ;
          rdfs:range vsd:TimeVaryingGraph ;
          rdfs:comment "Relates a named time varying graph to its graph description." .


###  https://w3id.org/rsp/vocals-sd#hasFeature
vsd:hasFeature rdf:type owl:ObjectProperty ;
               rdfs:domain vsd:RSService ;
               rdfs:range vsd:RSFeature ;
               rdfs:comment "Relates a vsd:RSService to an available feature" .


###  https://w3id.org/rsp/vocals-sd#namedTimeVaryingGraph
vsd:namedTimeVaryingGraph rdf:type owl:ObjectProperty ;
                          rdfs:domain vsd:SDS ;
                          rdfs:range vsd:TimeVaryingGraph ;
                          rdfs:comment "Relates an instance of vocals:SDS to the description of one of its named time varying graphs. The description of such a named time varying graph MUST include the vsd:name property." .


###  https://w3id.org/rsp/vocals-sd#publishedBy
vsd:publishedBy rdf:type owl:ObjectProperty ;
                rdfs:domain vocals:RDFStream ;
                rdfs:range vsd:PublishingService ;
                rdfs:comment "Points to an instance of vsd:PublishingService that publishs a certain vocals:RDFStream." .


###  https://w3id.org/rsp/vocals-sd#registerdTask
vsd:registerdTask rdf:type owl:ObjectProperty ;
                  rdfs:domain vsd:RSService ;
                  rdfs:comment "Relates an instance of vsd:RSService with instances of running tasks, e.g. RSP-QL Query ." ;
                  rdfs:label "registerd task" .


###  https://w3id.org/rsp/vocals-sd#registeredBy
vsd:registeredBy rdf:type owl:ObjectProperty ;
                 rdfs:domain vocals:RDFStream ;
                 rdfs:range vsd:ProcessingService ;
                 rdfs:comment "Points to an instance of vsd:ProcessingService that registers/ processes a certain vocals:RDFStream." .


###  https://w3id.org/rsp/vocals-sd#registeredStreams
vsd:registeredStreams rdf:type owl:ObjectProperty ;
                      rdfs:domain vsd:RSService ;
                      rdfs:range vocals:RDFStream ;
                      rdfs:comment "Relates an instance of vsd:RSService to the streams registered on it." .


###  https://w3id.org/rsp/vocals-sd#resultFormat
vsd:resultFormat rdf:type owl:ObjectProperty ;
                 rdfs:domain vsd:RSService ;
                 rdfs:range frmt:Format ;
                 rdfs:comment "Relates an instance of sd:Service to a format that is supported for serializing query results." ;
                 rdfs:label "result format" .


###  https://w3id.org/rsp/vocals-sd#rspEndpoint
vsd:rspEndpoint rdf:type owl:ObjectProperty ;
                rdfs:domain vsd:RSService ;
                rdfs:comment "The RSP-QL endpoint of an vocals:RSPService that implements the RSP-QL Protocol service. The object of the vocals:rspEndpoint property is an IRI." ;
                rdfs:label "RSP endpoint" .


###  https://w3id.org/rsp/vocals-sd#startedAt
vsd:startedAt rdf:type owl:ObjectProperty ;
              rdfs:domain vsd:Window ;
              rdfs:range time:Instant .


#################################################################
#    Classes
#################################################################

###  http://www.w3.org/2006/time#Instant
time:Instant rdf:type owl:Class .


###  http://www.w3.org/ns/dcat#Dataset
<http://www.w3.org/ns/dcat#Dataset> rdf:type owl:Class .


###  http://www.w3.org/ns/formats/Format
frmt:Format rdf:type owl:Class .


###  https://w3id.org/rsp/vocals#RDFStream
vocals:RDFStream rdf:type owl:Class .


###  https://w3id.org/rsp/vocals-sd#CatalogService
vsd:CatalogService rdf:type owl:Class ;
                   rdfs:subClassOf vsd:RSService ;
                   rdfs:comment "An vsd:CatalogService is a service storing stream descriptions. (e.g. SPARQL endpoint, RDF file with stream description,...etc)" ;
                   rdfs:label "Catalog service" ;
                   rdfs:seeAlso "vocals:StreamDescriptor" .


###  https://w3id.org/rsp/vocals-sd#Operation
vsd:Operation rdf:type owl:Class ;
              rdfs:subClassOf vsd:RSFeature ;
              rdfs:comment "An instance of vsd:Operation represents an available action/function that can be performed by vsd:RSService." ;
              rdfs:label "operation" .


###  https://w3id.org/rsp/vocals-sd#PhysicalWindow
vsd:PhysicalWindow rdf:type owl:Class ;
                   rdfs:subClassOf vsd:Window ;
                   rdfs:comment "Window defined by an interval size (in units of RDF elements)" .


###  https://w3id.org/rsp/vocals-sd#ProcessingService
vsd:ProcessingService rdf:type owl:Class ;
                      rdfs:subClassOf vsd:RSService ;
                      rdfs:comment "An vsd:ProcessingService represents an RDF stream processing service. Processing means any kind of transformation on streaming data, i.e., Querying, Reasoning, Filtering...etc" .


###  https://w3id.org/rsp/vocals-sd#PublishingService
vsd:PublishingService rdf:type owl:Class ;
                      rdfs:subClassOf vsd:RSService ;
                      rdfs:comment "An vsd:PublishingService represents a stream publishing service. Publishing has to be interpreted as in Linked Data domain, where is possible." .


###  https://w3id.org/rsp/vocals-sd#R2ROperation
vsd:R2ROperation rdf:type owl:Class ;
                 rdfs:subClassOf vsd:Operation ;
                 rdfs:comment "An instance of vsd:R2ROperation represent a realtion-to-relation operation that an vsd:RSService can perform. (e.g. filtering, aggregation)" ;
                 rdfs:label "relation-to-relation operation" .


###  https://w3id.org/rsp/vocals-sd#R2SOperation
vsd:R2SOperation rdf:type owl:Class ;
                 rdfs:subClassOf vsd:Operation ;
                 rdfs:comment "An instance of vsd:R2SOperation represent a realtion-to-stream operation that an vsd:RSService can perform. (e.g. replay a static dataset as a stream)" ;
                 rdfs:label "relation-to-stream operation" .


###  https://w3id.org/rsp/vocals-sd#RSFeature
vsd:RSFeature rdf:type owl:Class ;
              rdfs:comment "An instance of vsd:Feature represents a feature of a RDF Streaming service." ;
              rdfs:label "RDF Streaming Feature" .


###  https://w3id.org/rsp/vocals-sd#RSService
vsd:RSService rdf:type owl:Class ;
              rdfs:subClassOf vsd:StreamingService ;
              rdfs:comment "An instance of vsd:Service represents a RDF streaming service made available via RDF streaming protocol." ;
              rdfs:label "RDF streaming service" .


###  https://w3id.org/rsp/vocals-sd#ReportingPolicy
vsd:ReportingPolicy rdf:type owl:Class ;
                    rdfs:subClassOf vsd:RSFeature ;
                    rdfs:label "Reporting Policy" .


###  https://w3id.org/rsp/vocals-sd#S2ROperation
vsd:S2ROperation rdf:type owl:Class ;
                 rdfs:subClassOf vsd:Operation ;
                 rdfs:comment "An instance of vsd:R2ROperation represent a stream-to-relation operation that an vsd:RSService can perform. (e.g. windowing)" ;
                 rdfs:label "stream-to-relation operation" .


###  https://w3id.org/rsp/vocals-sd#S2SOperation
vsd:S2SOperation rdf:type owl:Class ;
                 rdfs:subClassOf vsd:Operation ;
                 rdfs:comment "An instance of vsd:R2ROperation represent a stream-to-stream operation that an vsd:RSService can perform. (e.g. transformation)" ;
                 rdfs:label "stream-to-stream operation" .


###  https://w3id.org/rsp/vocals-sd#SDS
vsd:SDS rdf:type owl:Class ;
        rdfs:comment "A streaming dataset is a set composed by an (optional) default graph, n (n ≥ 0) named graphs and m (m ≥ 0) named time-varying graphs" ;
        rdfs:label "SDS" .


###  https://w3id.org/rsp/vocals-sd#SetOperation
vsd:SetOperation rdf:type owl:Class ;
                 rdfs:subClassOf vsd:S2SOperation ;
                 rdfs:comment "An instance of vsd:SetOperation represents an available set operator that can be performed by vsd:RSService. (e.g. union, intersection)" ;
                 rdfs:label "Set operation" .


###  https://w3id.org/rsp/vocals-sd#StreamingService
vsd:StreamingService rdf:type owl:Class ;
                     rdfs:comment "An instance of vsd:StreamingService represents a streaming service made available via a certain streaming protocol." ;
                     rdfs:label "Streaming service" .


###  https://w3id.org/rsp/vocals-sd#TimeBasedWindow
vsd:TimeBasedWindow rdf:type owl:Class ;
                    rdfs:subClassOf vsd:Window ;
                    rdfs:comment "A time-based window is a window defined through two time instants o, c (respectively named opening and closing time instants) such that: W(S) = {d|(d, t) ∈ S ∧ t ∈ (o, c]}" .


###  https://w3id.org/rsp/vocals-sd#TimeControl
vsd:TimeControl rdf:type owl:Class ;
                rdfs:subClassOf vsd:RSFeature .


###  https://w3id.org/rsp/vocals-sd#TimeSemantics
vsd:TimeSemantics rdf:type owl:Class ;
                  rdfs:subClassOf vsd:RSFeature .


###  https://w3id.org/rsp/vocals-sd#TimeVaryingGraph
vsd:TimeVaryingGraph rdf:type owl:Class ;
                     rdfs:comment "A time-varying graph G is a function that relates time instants t ∈ T to RDF graphs: G : T → {g | g is an RDF graph}" .


###  https://w3id.org/rsp/vocals-sd#Window
vsd:Window rdf:type owl:Class ;
           rdfs:subClassOf <http://www.w3.org/ns/dcat#Dataset> ;
           rdfs:comment "A window W(S) is a set of RDF statements extracted from a stream S." .


